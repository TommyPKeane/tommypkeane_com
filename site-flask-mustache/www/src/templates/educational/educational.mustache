{{> page_head}}
    <main>
      <div id="intro_container" class="container-fluid page_intro">
        <div class="card mt-3 mb-3">
          <h3 class="card-title">Educational Pages</h3>
          <div class="card-body">
            {{{intro_content}}}
          </div>
          <a id="collapse-intro" href="#" class="btn btn-outline-success btn-lg" role="button" title="Click to collapse." data-toggle="tooltip" data-placement="bottom">{{{img_intro_collapse}}}</a>
          <a id="expand-intro" href="#" class="btn btn-outline-success btn-lg" role="button" title="Click to expand." data-toggle="tooltip" data-placement="bottom">{{{img_intro_expand}}}</a>
        </div>
      </div>
      <div class="container-fluid">
        {{#educational_links}}
          <a class="educational_a" href="{{page_link}}">
            <div class="card educational_link">
              <img class="card-img-top" src="{{img_src}}" alt="{{img_alt}}">
              <div class="card-body">
                <h4 class="card-title">{{card_title}}</h4>
                <p>{{{card_desc}}}</p>
              </div>
            </div>
          </a>
        {{/educational_links}}
      </div>
      <a id="back-to-top" href="#" class="btn btn-danger btn-lg back-to-top" role="button" title="Click to return on the top page" data-toggle="tooltip" data-trigger="hover focus" data-placement="left" data-html="true">{{{img_scroll_to_top}}}</a>
    </main>
{{> page_foot}}