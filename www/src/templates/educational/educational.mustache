{{> page_head}}
    <main>
      <div id="intro_container" class="collapsable billet intro">
        <div class="billet-header">
          <h3>Educational Pages</h3>
        </div>
        <div class="billet-body">
          {{{intro_content}}}
        </div>
        <a id="collapse-intro" href="#" class="bouton fill" role="button" title="Click to collapse." data-toggle="tooltip" data-placement="bottom">{{{img_intro_collapse}}}</a>
        <a id="expand-intro" href="#" class="bouton fill" role="button" title="Click to expand." data-toggle="tooltip" data-placement="bottom">{{{img_intro_expand}}}</a>
      </div>
      <div class="">
        {{#educational_links}}
          <a class="educational_a" href="{{page_link}}">
            <div class="billet educational_link">
              <img class="billet-img-top" src="{{img_src}}" alt="{{img_alt}}">
              <div class="billet-body">
                <h4 class="billet-title">{{card_title}}</h4>
                <p>{{{card_desc}}}</p>
              </div>
            </div>
          </a>
        {{/educational_links}}
      </div>
      <a id="back-to-top" href="#" class="btn btn-danger btn-lg back-to-top" role="button" title="Click to return on the top page" data-toggle="tooltip" data-trigger="hover focus" data-placement="left" data-html="true">{{{img_scroll_to_top}}}</a>
    </main>
{{> page_foot}}
